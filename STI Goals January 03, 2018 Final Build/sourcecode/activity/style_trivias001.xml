<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/style_theforbiddenonea"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="0dp"
android:paddingLeft="0dp"
android:paddingRight="0dp"
android:paddingTop="0dp"
    android:background="@raw/background001"
tools:context="pro.gr.ams.stigoals.trivias001">

    <android.support.v4.view.ViewPager
        android:id="@+id/viewPager"
        android:layout_width="match_parent"
        android:layout_height="437dp"
        android:layout_alignParentBottom="true">

    </android.support.v4.view.ViewPager>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="140dp"
        app:srcCompat="@raw/trivias_logo001"
        android:id="@+id/imageView2"
        android:layout_marginTop="16dp"
        android:layout_alignTop="@+id/gotohome001"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/gotohome001"
        android:background="@raw/back001"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="16dp"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentEnd="true" />


</RelativeLayout>

